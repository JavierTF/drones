generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model medication {
  id               Int                @id @default(autoincrement())
  name             String             @unique(map: "name") @db.Char(150)
  weight           Float              @db.Float
  code             String             @db.Char(50)
  image            String?            @db.VarChar(255)
  drone_medication drone_medication[]
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model model {
  id                       Int     @id @default(autoincrement())
  name                     String  @db.Char(15)
  drone_drone_modelTomodel drone[] @relation("drone_modelTomodel")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model state {
  id                       Int     @id @default(autoincrement())
  name                     String  @db.Char(15)
  drone_drone_stateTostate drone[] @relation("drone_stateTostate")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model drone {
  id                       Int                @id @default(autoincrement())
  serial_number            String             @unique(map: "serial_number") @db.Char(100)
  model                    Int
  weight_limit             Float              @default(0) @db.Float
  battery_capacity         Float              @db.Float
  state                    Int
  model_drone_modelTomodel model              @relation("drone_modelTomodel", fields: [model], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK_dron_model")
  state_drone_stateTostate state              @relation("drone_stateTostate", fields: [state], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK_drone_state")
  drone_log                drone_log[]
  drone_medication         drone_medication[]

  @@index([state], map: "state")
  @@index([model], map: "FK_drone_model")
}

model drone_log {
  id          Int       @id @default(autoincrement())
  drone_id    Int
  battery_log Int
  update      DateTime? @db.DateTime(0)
  drone       drone     @relation(fields: [drone_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK_drone_log_drone")

  @@index([drone_id], map: "FK__dron")
}

model drone_medication {
  id            Int        @id @default(autoincrement())
  drone_id      Int
  medication_id Int
  timelog       DateTime?  @db.DateTime(0)
  drone         drone      @relation(fields: [drone_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK_drone_medication_drone")
  medication    medication @relation(fields: [medication_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK_drone_medication_medication")

  @@index([drone_id], map: "FK_dron_medication_dron")
  @@index([medication_id], map: "FK_drone_medication_medication")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model config {
  id    Int    @id @default(autoincrement())
  key   String @db.Char(50)
  value String @db.Char(50)
}
